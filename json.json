		"prefix": "optiContenType",
		"description": "A content type definition",
		"body": [
			"{",
			"  \"id\": \"${1:guid}\",",
			"  \"name\": \"${2:Name of the content type}\",",
			"  \"baseType\": \"${3:page OR block OR folder}\",",
			"  \"version\": \"1.0.0\",",
			"  \"supportedMediaExtensions\": null,",
			"  \"editSettings\": {",
			"        \"displayName\": \"${4:Display name}\",",
			"        \"groupName\": \"${5:Group name}\",",
			"        \"description\": \"${6:Description}\",",
			"        \"available\": true,",
			"        \"sortOrder\": 0",
			"  },",
			"  \"properties\": [",
			"    $0",
			"  ]",
			"}",
		]
